import re

mylist = ["А123АА11", "А222АА123", "A12AA123", "A123CC1234", "AA123A12", "Т123РР12", "Т000РР12"]
#r = re.compile("^([АВЕКМНОРСТУХавекмнорстухABEKMHOPCTYXabekmhopctyx]\s*\d{3}\s*[АВЕКМНОРСТУХавекмнорстухABEKMHOPCTYXabekmhopctyx]{2}\s*\d{2,3}$)|([АВЕКМНОРСТУХавекмнорстухABEKMHOPCTYXabekmhopctyx]{2}\s*\d{3}\s*\d{2,3}$)")
r = re.compile("^[АВЕКМНОРСТУХ]\d{3}(?<!000)[АВЕКМНОРСТУХ]{2}\d{2,3}$")
newlist = list(filter(r.match, mylist))
print(newlist)
